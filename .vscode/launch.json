	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch API Backend (CC) Server", 
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/backend/node_modules/**"
			],
			"cwd": "${workspaceFolder}/backend",
			"program": "${workspaceFolder}/backend/build/index.js",
			"outFiles": [
				"${workspaceFolder}/backend/build/**/*.js",
			],
			"console": "integratedTerminal",
			"preLaunchTask": "npx tsc build backend"
		}
	]
}